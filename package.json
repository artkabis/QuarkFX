  "engines": {
      "node": ">=14.0.0"
  },
  "browserslist": [
      ">0.2%",
      "not dead",
      "not op_mini all"
  ],
  "devDependencies": {
      "@rollup/plugin-babel": "^6.0.4",
      "@rollup/plugin-terser": "^0.4.4",
      "rollup": "^4.29.1",
